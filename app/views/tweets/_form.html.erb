<%= form_for @tweet do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div><br>

  <div>
    <%= f.label :email %>
    <%= f.text_field :email%>
  </div><br>

  <div>
    <%= f.label :content %>
    <%= f.text_field :content %>
  </div><br>

  <%= f.submit %> <br><br>

<% end %>
